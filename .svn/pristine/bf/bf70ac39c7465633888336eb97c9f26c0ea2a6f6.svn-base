
body
{
	margin: 0px;
	padding: 10px;
}

body, td, input, select, textarea
{
	font-size: 11px;
	font-family: 'Microsoft Sans Serif' , Arial, Helvetica, Verdana;
}

body, .BackColor
{
	background-color: #F2F3FF;
}

.PopupBody
{
	margin: 0px;
	padding: 0px;
}

.PopupTitle
{
	font-weight: bold;
	font-size: 14pt;
	color: #000066;
	background-color: #DADAF3;
	padding: 3px 10px 3px 10px;
}

.PopupButtons
{
	border-top: #C8C8FE 1px solid;
	background-color: #DADAF3;
	padding: 7px 10px 7px 10px;
}

.Button
{
	border: #737357 1px solid;
	color: #3b3b1f;
	background-color: #EEEEEE;
}

#btnOk
{
	width: 80px;
}

.DarkBackground
{
	background-color: #DADAF3;
}

.LightBackground
{
	background-color: #F1F1FF;
}

.PopupTitleBorder
{
	border-bottom: #C8C8FE 1px solid;
}

.PopupTabArea
{
	color: #000066;
	background-color: #D4D4F7;
}

.PopupTabEmptyArea
{
	padding-left: 10px ;
	border-bottom: #C8C8FE 1px solid;
}

.PopupTab, .PopupTabSelected
{
	border-right: #C8C8FE 1px solid;
	border-top: #C8C8FE 1px solid;
	border-left: #C8C8FE 1px solid;
	padding-right: 5px;
	padding-left: 5px;
	padding-bottom: 3px;
	padding-top: 3px;
	color: #000066;
}

.PopupTab
{
	margin-top: 1px;
	border-bottom: #C8C8FE 1px solid;
	cursor: pointer;
	cursor: hand;
}

.PopupTabSelected
{
	font-weight:bold;
	cursor: default;
	padding-top: 4px;
	border-bottom: #F1F1FF 1px solid;
	background-color: #F1F1FF;
}

.PopupSelectionBox
{
	border: #ff9933 1px solid !important;
	background-color: #fffacd !important;
	cursor: pointer;
	cursor: hand;
}