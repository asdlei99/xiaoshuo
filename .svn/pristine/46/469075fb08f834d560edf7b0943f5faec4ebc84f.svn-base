INSERT INTO `xs_archives_4` VALUES ('168', '', '<p>\r\n	雾雪的奇妙忍界生活，不同帅哥的不同恋爱史，<br />\r\n	　　回到最初，还是选择真爱，血仇，平息，我是修罗，我是雾雪，<br />\r\n	　　你们对我做过的事，我会一个不剩的还回去，我所承受的痛苦，你们将承受的更多，<br />\r\n	　　父母的恩怨，自己的情感，不同的羁绊，抱歉，我爱你们，可是，我的爱只能给一个人。<br />\r\n	　　每天早上：8：:00准时更新。<br />\r\n	　　联系企鹅号：854514968<br />\r\n	　　联系企鹅群：286014249</p>');
