.h117{ height:117px;}
#header{ margin:0 auto; width:100%; min-width:990px; height:75px; clear:both; background:#134d9d;}
#header{_width:expression((document.documentElement.clientWidth||document.body.clientWidth)<990?"990px":"100%");}
#header .logo{ float:left;width:165px; height:75px; text-align:left;}
#header .logo img { width:165px; height:75px; overflow:hidden;}
#header .version { float:left; width:60px; height:25px; text-align:left; color:#efefef; margin-top:45px; font-size:14px; font-weight:600; padding-left:5px;}
#header .headTxt { width:auto; min-width:760px; color:#efefef; font-size:14px; text-align:right;}
#header .headTxt h1{ height:33px; line-height:33px; text-align:right; padding-right:12px;}
#header .headTxt h1 font { color:#efefef; padding-right:12px; font-size:12px; }
#header .headTxt .commonActions{ float:right; width:760px; height:42px; line-height:42px; text-align:right; padding-right:12px; overflow:hidden;}
#header .headTxt .commonActions ul { padding-right:12px;}
#header .headTxt .commonActions ul li { float:right; padding:0 5px;}
#header .headTxt .commonActions h2 { float:right; width:100px; height:22px; padding-top:9px; display:block;}
#header .headTxt .commonActions a:link,#header .headTxt h1 a:link {color: #efefef;}
#header .headTxt .commonActions a:hover,#header .headTxt h1 a:hover {color: #efefef; text-decoration:underline;}
#header .headTxt .commonActions a:active,#header .headTxt h1 a:active {color: #efefef;}
#header .headTxt .commonActions a:visited,#header .headTxt h1 a:visited {color: #CCC; font-weight:600;text-decoration:none;}
#globalNav{ float:left; width:100%; height:42px; clear:both; background:url(globalNav_right.png) repeat-x;}
#globalNav h1{ float:left; width:186px; height:42px; line-height:42px; text-align:left; padding:0 0 0 38px; background:url(globalNav_left.png) no-repeat;}
#globalNav h1 div { width:120px; overflow:hidden;}
#globalNav h1 select { width:120px; height:23px; padding-top:2px; border:0; background:url(select_bg.gif) no-repeat; margin:8px 0 0 0;}
#globalNav ul { float:left; margin-top:6px;}
#globalNav ul li{ float:left; margin:0 2px; width:71px; height:36px; line-height:36px; color:#666; background:url(nav_link.gif) no-repeat;}
#globalNav ul li.s1{ float:left; width:71px; height:36px; line-height:36px; color:#134d9d; font-weight:600; background:url(nav_visited.gif) no-repeat;}
#globalNav ul li a:hover { padding:10px 11px 13px; *padding:10px 11px 13px; _padding:11px 11px 12px; background:url(nav_hover.gif) no-repeat; text-decoration:none; white-space:nowrap; overflow:hidden}
#globalNav ul li.s1 a:hover {text-decoration:none; background:none; padding:0;}
