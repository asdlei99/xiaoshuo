#btnSubmit { margin: 0 0 0 155px ; }

/* -- Table Styles ------------------------------- */
td {
	font: 12px Helvetica, Arial, sans-serif;
	vertical-align: top;
}
input,select,button {
	font-size:12px;
}
.progressWrapper {
	width: 580px;
	overflow: hidden;
}

.progressContainer {
	margin-top: 5px;
	padding:5px;
	width: 568px;
	border: solid 1px #E8E8E8;
	background-color: #F7F7F7;
	overflow: hidden;
}
/* Message */
.message {
	margin: 1em 0;
	padding: 10px 20px;
	border: solid 1px #FFDD99;
	background-color: #FFFFCC;
	overflow: hidden;
}
/* Error */
.red {
	border: solid 1px #B50000;
	background-color: #FFEBEB;
}

/* Current */
.green {
	border: solid 1px #DDF0DD;
	background-color: #EBFFEB;
}

/* Complete */
.blue {
	border: solid 1px #CEE2F2;
	background-color: #F0F5FF;
}

.progressName {
	font-size: 12px;
	font-weight: 700;
	color: #555;
	width: 550px;
	height: 14px;
	text-align: left;
	white-space: nowrap;
	overflow: hidden;
}

.progressBarInProgress,
.progressBarComplete,
.progressBarError {
	font-size: 0;
	width: 0%;
	height: 2px;
	background-color: blue;
	margin-top: 2px;
}

.progressBarComplete {
	width: 100%;
	background-color: green;
	visibility: hidden;
}

.progressBarError {
	width: 100%;
	background-color: red;
	visibility: hidden;
}

.progressBarStatus {
	margin-top: 2px;
	width: 570px;
	font-size: 12px;
	font-family: Arial;
	text-align: left;
	white-space: nowrap;
}

a.progressCancel {
	font-size: 0;
	display: block;
	height: 14px;
	width: 14px;
	background-image: url(cancelbutton.gif);
	background-repeat: no-repeat;
	background-position: -14px 0px;
	float: right;
}

a.progressCancel:hover {
	background-position: 0px 0px;
}


/* -- SWFUpload Object Styles ------------------------------- */
.swfupload {
	vertical-align: top;
	position: absolute;
	z-index: 1;
}

#loading{
	width:200px;
	margin:50px auto;
	text-align:center;
}

.clear{
	clear:both;
}

.remark{
	background-color: #FFFF66;
	border-top: solid 4px #FF9966;
	border-bottom: solid 4px #FF9966;
	margin: 10px 25px;
	padding: 10px 15px;
}

.selecttable{
	margin:10px auto;
}
.selectitem{
	background:url(button.gif) 0px 21px repeat-x;
}
.selectitem a{
	display:block;
	float:left;
	padding:5px;
	width:52px;
	height:12px;
	line-height:16px;
	overflow:hidden;
	color:#000;
	background:url(button.gif) no-repeat;
}
.selectitem a,a.selectitem0{
	background-position:-62px -1px;
}
a.selecteditem,.selectitem a:hover{
	background-position:0px -1px;
	text-decoration:none;
}
a.selectitem1{
	background-position:-124px -1px;
}
a.endselectitem,a.endselectitem:hover{
	background-position:-186px -1px;
}
.selectbody{
	border:1px solid #999;
	border-top:none;
}
.selectbody p{
	margin-left:5px;
}
.oldupload{
	text-indent:5px;
	margin-top:5px;
	border:solid 1px #eee;
}
.oldupload .submit,.oldupload .button{
	width:60px;
	height:21px;
}
.oldupload .path,.rmturl{
	width:350px;
	height:15px;
}
.oldupload .text{
	width:400px;
	height:15px;
}
.oldupload .filename{
	border:none;
	width:400px;
}
.rmtform{
	position:relative;
	padding-top:8px;
}
#rmtpath{
	width:450px;
	height:200px;
}
.rmtnum{
	width:50px;
	height:15px;
}
.iepadding{
	font-size:0px;
}